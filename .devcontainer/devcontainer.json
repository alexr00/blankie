{
	"name": "cmbrose/blankie",
		
	"postCreateCommand": "sleep 10 && cd /workspaces/blankie/test && dotnet run",
    
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
	},

	"runArgs": ["--privileged", "--security-opt", "seccomp=unconfined"],
	"overrideCommand": false,
	
	"forwardPorts": [
		5000, 
		5001
	],
	"appPort": [
		5000, 
    	5001
	],
	"portsAttributes": {
		"5000": {
			"label": "http"
		},
		"5001": {
			"label": "https"
		}
	},
}
